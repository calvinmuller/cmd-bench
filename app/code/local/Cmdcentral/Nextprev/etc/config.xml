<?xml version="1.0"?>
<config>
  <modules>
    <Cmdcentral_Nextprev>
      <version>0.1.0</version>
    </Cmdcentral_Nextprev>
  </modules>
  <frontend>
    <routers>
      <nextprev>
        <use>standard</use>
          <args>
            <module>Cmdcentral_Nextprev</module>
            <frontName>nextprev</frontName>
          </args>
      </nextprev>
    </routers>
	<layout>
      <updates>
        <nextprev>
          <file>nextprev.xml</file>
        </nextprev>
      </updates>
    </layout>
  </frontend>
  <global>
    <helpers>
      <nextprev>
        <class>Cmdcentral_Nextprev_Helper</class>
      </nextprev>
    </helpers>
	<blocks>
	  <nextprev>
		<class>Cmdcentral_Nextprev_Block</class>
	  </nextprev>
	</blocks>
  </global>
</config> 
